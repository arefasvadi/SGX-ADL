{
    "cmake.autoRestartBuild": true,
    "cmake.preferredGenerators": [
        "Unix Makefiles",
        "Ninja"
    ],
    //"cmake.configureEnvironment": {"SGX_SDK":"/opt/intel/sdk-2.7/sgxsdk",},
    "cmake.configureEnvironment": {"SGX_SDK":"/opt/intel/sdk-2.7-debug/sgxsdk",},
    "cmake.parallelJobs": 7,
    "python.linting.pylintEnabled": true,
    "python.pythonPath": "/usr/bin/python",
    "python.linting.enabled": false,
    "files.associations": {
        "map": "cpp",
        "array": "cpp"
    },
    "ccls.index.multiVersion": 1,
    "ccls.highlighting.bold.templateParameters": false,
    "ccls.highlighting.bold.typeAliases": false,
    "ccls.highlighting.bold.types": false,
    "ccls.highlighting.bold.namespaces": false,
    "ccls.highlighting.bold.enumConstants": false,
    "ccls.highlighting.bold.enums": false,
    "python.formatting.provider": "yapf",
    //"cmake.copyCompileCommands": "${workspaceRoot}/"
}